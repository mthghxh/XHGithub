$(function(){
    initUIElements();

    $("#expertTeamUpdateForm").submit(function(event) {
        var popWin = document.getElementById("popWin");
        if(popWin != null){
            $("#searchBtn").trigger("click");
            return false;
        }
        var hospital_item = $("#hospitalSelect").jqxDropDownList('getSelectedItem');
        var hospital_guid = null;
        if(hospital_item != null) {
            hospital_guid = hospital_item.value;
        }
        var doctor_guid = $("#doctorGuid").val();
        var team_name = $("#teamName").val();
        var team_dept = $("#department").val();
        var team_skill = $("#teamSkill").val();
        var team_intro = $("#teamIntroduction").val();
        var team_contact = $("#phoneNum").val();
        var team_avatar = $("#doctorAvatarImg").attr("src");

        if (hospital_guid.length == 0) { alert("【所属医院】未选择"); return false;}
        if (doctor_guid.length == 0) { alert("【团队首席】未选择"); return false;}
        else {
            var paramsArray = [];
            paramsArray.push({name:"【团队名称】", value:team_name});
            paramsArray.push({name:"【联系方式】", value:team_contact});
            paramsArray.push({name:"【团队擅长】", value:team_skill});
            paramsArray.push({name:"【团队简介】", value:team_intro});
            paramsArray.push({name:"【擅长科室】", value:team_dept});

            var errorMessage = '';
            for(var i = 0; i < paramsArray.length; i++) {
                if($.isBlank($.trim(paramsArray[i].value))) {
                    errorMessage += paramsArray[i].name + "不能为空 \n";
                }
            }
            if(!$.isBlank(errorMessage)) {
                alert(errorMessage);
                return false;
            }
            var team_skills = new Array();
            team_skill = team_skill.replace(/，/g, ",")
            var len = team_skill.length;
            if(team_skill.lastIndexOf(",") == len-1){
                team_skill = team_skill.substring(0, len-1);
            }
            team_skills = team_skill.split(",");
            for(var i=0;i<team_skills.length;i++){
                if($.isBlank($.trim(team_skills[i]))){ alert("【团队擅长】输入不规范,请检查.");return false;}
            }

            if (team_name.length > 12) { alert("【团队名称】最多只能输入12个字符"); return false;}
            if (team_dept.length > 10) { alert("【擅长科室】最多只能输入10个字符"); return false;}
            if (team_skills.length > 10) { alert("【团队擅长】最多只能输入10个关键词"); return false;}

            var params = "hospitalGuid=" + hospital_guid;
            params += "&doctorGuid="     + doctor_guid;
            params += "&teamAvatar="     + team_avatar;
            params += "&teamName="       + encodeURIComponent($.trim(team_name));
            params += "&phoneNum="       + encodeURIComponent($.trim(team_contact));
            params += "&skills="         + encodeURIComponent($.trim(team_skill));
            params += "&introduction="   + encodeURIComponent($.trim(team_intro));
            params += "&departments="    + encodeURIComponent($.trim(team_dept));
            showLoadingHs();

            $.post(
                "/expert/perform-create-update-expert-team",
                params,
                function(response) {
                    hideLoadingHs();
                    if(response.commonMessage == 'privilege_error') {
                        alert("您没有足够的权限执行当前操作，\n请先登录, 或用其他具备当前操作权限的用户重新登录");
                        window.location.reload(true);
                        return;
                    }
                    alert("数据提交成功");
                    window.location.href = '/expert/show-expert-team-list' ;
                },
                'json'
            ).error(function() {
                hideLoadingHs();
                alert("系统访问出错，可能是您的网络问题，请稍后再试");
            });
        }
        return false;
    });
});

function initUIElements() {

    $(".taoguBtn").tg_jqxButton({ theme: "taogubutton", width: 100, height: 30 });
    $("#searchBtn").jqxToggleButton({ theme:'taogubutton', width: 60, height:30});
    $("#subDepartmentId").append($("<option></option>").attr("value", "").text("二级科室"));
    $("#mainDepartmentId").append($("<option></option>").attr("value", "").text("一级科室"));
    $("#hospitalGuid, #hospitalSelect").tg_jqxDropDownList({ width: 230, height: 30 });
    $("#teamSkill, #teamIntroduction").val("");

    initDoctorGrid();

    $("#hospitalSelect").on("change",function() {
        var hospital_item = $("#hospitalSelect").jqxDropDownList('getSelectedItem');

        for(var i=0; i<hospitals.length; i++) {

            if(hospitals[i].guid == hospital_item.value) {
                $("#hospitalGuid").jqxDropDownList('selectIndex', i+1);
                break;
            }
        }
        fillMainDepartment(hospital_item.value);

        //$("#hospitalGuid").tg_jqxDropDownList({enableSelection : false});
    });

    $("#mainDepartmentId").on("change",function(){
        var mainDepartmentId = $(this).val();
        var hospital_item = $("#hospitalGuid").jqxDropDownList('getSelectedItem');
        var hospitalGuid = null;
        if (hospital_item != null) {
            hospitalGuid = hospital_item.value;
        }
        if(mainDepartmentId.length == 0){
            $("#subDeptSearch").val("");
        }
        
        fillSubDepartment(hospitalGuid, mainDepartmentId);
    });

    $("#subDepartmentId").on("change",function() {
        $("#subDeptSearch").val(document.all.subDepartmentId.value);
    });
}

function fillMainDepartment(hospitalGuid) {
    //query-main-department
    var params = 'hospitalGuid=' + hospitalGuid;

    showLoadingHs();
    $.post(
        "/expert/query-main-department",
        params,
        function(response) {
            hideLoadingHs();
            if(response.commonMessage == 'privilege_error') {
                alert("您没有足够的权限执行当前操作，\n请先登录, 或用其他具备当前操作权限的用户重新登录");
                window.location.reload(true);
                return;
            }
            if(response.commonMessage == 'success') {
                var departments = $.parseJSON(response.mainDepartmentsJson);

                $("#mainDepartmentId option").remove();

                var mainDepartmentCtrl = $("#mainDepartmentId");
                mainDepartmentCtrl.append($("<option></option>").attr("value", "").text("一级科室"));

                for(var i = 0; i < departments.length; i++) {
                    var curDep = departments[i];
                    var depId = curDep.id;
                    var depName = curDep.name;
                    mainDepartmentCtrl.append($("<option></option>").attr("value", depId).text(depName));
                }
            } else {
                alert(response.commonMessage);
            }
        },
        'json'
    ).error(function() {
        hideLoadingHs();
        alert("系统访问出错，可能是您的网络问题，请稍后再试1");
    });
}

function fillSubDepartment(hospitalGuid, mainDepartmentId) {
    var params = "mainDepartmentId=" + mainDepartmentId;
    params += "&hospitalGuid=" + hospitalGuid;
    showLoadingHs();
    $.post(
        "/common/retrieve-subdepartments-by-mainDepartmentId",
        params,
        function(response){
            hideLoadingHs();
            if(response.commonMessage == 'privilege_error') {
                alert("您没有足够的权限执行当前操作，\n请先登录, 或用其他具备当前操作权限的用户重新登录");
                window.location.reload(true);
                return;
            }
            if(response.commonMessage == 'success') {
                var departments = $.parseJSON(response.departmentsJson);

                $("#subDepartmentId option").remove();

                var subDepartmentCtrl = $("#subDepartmentId");
                subDepartmentCtrl.append($("<option></option>").attr("value", "").text("二级科室"));

                for(var i=0; i<departments.length; i++) {
                    var curDep = departments[i];
                    var depId = curDep.id;
                    var depName = curDep.name;
                    subDepartmentCtrl.append($("<option></option>").attr("value", depId).text(depName));
                }
            } else {
                alert(response.commonMessage);
            }
        },
        'json'
    ).error(function() {
        hideLoadingHs();
        alert("系统访问出错，可能是您的网络问题，请稍后再试1");
    });
}

