<?xml version="1.0" ?>  
<!DOCTYPE hibernate-mapping PUBLIC    
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="test.login" table="xhpatient">
		<id name="id" column="guid" type="int">
			<generator class="increment" />
		</id>

		<property name="telphoneID" column="telphoneId" type="String" />
		<property name="deltime" column="deltime" type="String" />
		<property name="delname" column="delName" type="String" />
	</class>
</hibernate-mapping>  
